<div id="pnl_editbankgrid">
	<div class="fgta-page-title">Daftar Bank</div>
	<div id="pnl_editbankgrid-title" style="margin-top: 0px; margin-bottom: 30px; font-weight: bold; font-size: 1.2em">XXXXXXX</div>


	<div style="margin-top: 10px; margin-bottom: 15px">
		<table id="pnl_editbankgrid-tbl_list" paging="true" cellspacing="0" width="100%">
			<thead>
				<tr>
					<th type="check">chk</th>
					<th mapping="partnerbank_id">partnerbank_id</th>
					<th mapping="partnerbank_accnum">partnerbank_accnum</th>
					<th mapping="partnerbank_accname">partnerbank_accname</th>
